  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OrgId">
            <Value>=Parameters!OrgId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustID">
            <Value>=Parameters!CustID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SiteID">
            <Value>=Parameters!SiteID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT  
 A.Customer_ID, B.Site_Use_ID, A.Customer_No as Customer_No ,
  B.Location, B.Customer_Name, A.[Address],A.City, B.Postal_Code, A.Cust_Status,
   B.Customer_Barcode, B.Dept,A.Customer_Type,A.Credit_Limit,A.Credit_Hold,A.Customer_Class,A.Bill_Credit_Period,
   A.Customer_OD_Status,A.Cash_Cust,A.Allow_FOC,C.[Description] 
   as Customer_Segment,D.[Description] as Sales_District, 
   b.Cust_status as ShipStat,A.Customer_No+'-'+ B.Customer_Name as customer,cast(A.Customer_ID as varchar)+'$'+ cast(B.Site_Use_ID as varchar) as ID,
   case when Credit_Hold='Y' OR  A.Cust_Status='N' then 'BLOCKED' else case when A.Cash_Cust='Y' then 'CASH' else 'NORMAL' end end as CustStatus,
(select Attrib_Value from TBL_Customer_Addl_Info CustInfo where CustInfo.Customer_ID=A.Customer_ID and CustInfo.Site_use_ID=A.Site_use_ID and CustInfo.Attrib_Name='VAT_REGISTRATION' ) as TRN
   FROM 
   dbo.app_GetOrgCustomers(@OrgId) AS A
 
 INNER JOIN
  TBL_Customer_Ship_Address_Map AS Q
ON  A.Customer_ID = Q.Inv_To_Customer_ID AND 
                       A.Site_Use_ID = Q.Inv_To_Site_ID  INNER JOIN
dbo.app_GetOrgCustomerShipAddress(@OrgId) AS B
ON Q.Ship_To_Customer_ID = B.Customer_ID AND 
Q.Ship_To_Site_ID = B.Site_Use_ID 
LEFT JOIN TBL_Customer_Segments As C ON B.Customer_Segment_ID=C.Customer_Segment_ID LEFT JOIN TBL_Sales_District As D ON B.Sales_District_ID=D.Sales_District_ID where
A.Customer_ID=@CustID and B.Site_Use_ID=@SiteID  order by B.Customer_Name Asc</CommandText>
      </Query>

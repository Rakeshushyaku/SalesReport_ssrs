  <DataSets>
    <DataSet Name="dsCustomerVisitDetails">
      <Query>
        <DataSourceName>DS_SWX_VS_BO</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RowID">
            <Value>=Parameters!RowID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select TOP 1 (C.Customer_Name+'-'+ISNULL(C.Location,'N/A')) As Customer_Name, C.Customer_No ,
C.City,
A.Customer_ID, A.Site_Use_ID,A.Visit_Start_Date,A.Visit_End_Date,
 A.SalesRep_ID,A.Scanned_Closing,A.Text_Notes, A.Voice_Notes,
  D.SalesRep_Name,ISNULL(A.Odo_Reading,'') As Odo_Reading,
    dbo.GetEmpName(A.Emp_Code) as Emp_Name ,CC_Name,CC_TelNo,
    (Select Customer_No+'-'+ Customer_Name from TBL_Customer_Ship_Address where Site_Use_ID=A.Cash_Site_ID and Customer_ID=A.Cash_Customer_ID 
   ) as Credit_Cust_Name,Cash_Customer_ID ,
    (SELECT COUNT(*) FROm TBL_Distribution_Check WHERE Visit_ID =A.Actual_Visit_ID)AS DC,
    (SELECT COUNT(*) FROm TBL_Order WHERE Visit_ID =A.Actual_Visit_ID)AS[Order],
    (SELECT COUNT(*) FROm TBL_RMA WHERE Visit_ID =A.Actual_Visit_ID)AS [Return],
(select Control_Value  from TBL_App_Control where Control_Key = 'SHOW_APPROVAL_CODE')AS AppCodeVisible,
(SELECT UserName FROM TBL_User WHERE User_ID in(SELECT  Approved_By
 FROM TBL_Approval_Codes WHERE Custom_Attribute_2 =@RowID
AND Used_For='RA' AND Custom_Attribute_2&lt;&gt;'X' ))
As ApprovedBy,

(SELECT Approval_Code 
 FROM TBL_Approval_Codes WHERE Custom_Attribute_2 =@RowID AND Custom_Attribute_2&lt;&gt;'X' 
AND Used_For='RA')
As ApprovalCode,


(SELECT  Y.Code_Description  FROM TBL_Approval_Codes AS X INNER JOIN TBL_App_Codes AS Y
ON X.Used_For =Y.Code_Value  WHERE X.Custom_Attribute_2 =@RowID AND Y.Code_Type='JUMP_ROUTE_ADHERANCE')AS
UsedFor
    
    from 
    TBL_FSR_Actual_Visits as A, TBL_Customer_Ship_Address as C, TBL_FSR as
     D where A.Customer_ID=C.Customer_ID AND A.Site_Use_ID=C.Site_Use_ID AND
      A.SalesRep_ID=D.SalesRep_ID AND A.Actual_Visit_ID=@RowID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
